[alias]
  cm = commit
  co = checkout
  rhh = reset HEAD --hard
  rv = remote -v
  st = status

[commit]
  gpgsign = true

[fetch]
	prune = true

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[gpg]
  format = ssh

[gpg "ssh"]
  program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"

